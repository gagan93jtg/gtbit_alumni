<% if !@success.nil? && @success.empty? %>
$('.error_update_password').html("<%= @error %>")
$('.success_update_password').html("")
<% elsif !@error.nil? && @error.empty? %>
$('.success_update_password').html("<%= @success %>")
$('.error_update_password').html("")
<% else %>
$('.error_update_password').html("Ohh ! We encountered an error. Try again later :) ")
$('.success_update_password').html("")
<% end %>
